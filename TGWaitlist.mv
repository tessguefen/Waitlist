	<MvELSEIF EXPR = "{ l.function EQ 'Waitlist_Add' }">			<MvFUNCTIONRETURN VALUE = "{ JSON_API_Waitlist_Add( l.module ) }">
	</MvIF>

	<MvFUNCTIONRETURN VALUE = "{ [ g.Module_JSON ].JSON_Response_Error( '#Error#', 'Invalid function' ) }">
